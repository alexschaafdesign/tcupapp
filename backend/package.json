    "@supabase/supabase-js": "^2.47.10",
    "axios": "^1.7.9",
    "cloudinary": "^1.41.3",
    "concurrently": "^9.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-jwt": "^8.5.1",
    "express-oauth2-jwt-bearer": "^1.6.0",
    "googleapis": "^144.0.0",
    "jwks-rsa": "^3.1.0",
